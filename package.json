    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-hot-loader": "1.4.4",
    "css-loader": "6.8.1",
    "csso-webpack-plugin": "2.0.0-beta.3",
    "express": "4.18.2",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.5.4",
